}, {
"scene": "res://scenes/level_parts/checkpoint.tscn"
}, {
"scene": "res://scenes/level_parts/jump_pad.tscn"
}, {
"scene": "res://addons/LinkedCamera2D/linked_camera.tscn"
}, {
"scene": "res://scenes/level_parts/rooms/room_changer.tscn"
}],
"scroll": 0
}, {
"name": "enemies",
"scenes": [],
"scroll": 0
}, {
"name": "friezes",
"scenes": [{
"scene": "res://sprites/friezesets/bluebox/bluebox0.tscn"
}, {
"scene": "res://sprites/friezesets/bluebox/decobox0.tscn"
}],
"scroll": 0
}, {
"name": "other",
"scenes": [{
"scene": "res://scenes/components/entity_component_system.tscn"
}],
"scroll": 0
}]
instance_dock/preview_resolution=Vector2i(200, 200)
instance_dock/movie_file=""
instance_dock/mjpeg_quality=1.0

[application]

config/name="CabraMayhem"
config/tags=PackedStringArray("2d")
run/main_scene="res://scenes/menus/main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0, 0.160784, 0.196078, 0)
boot_splash/image="res://sprites/entities/boy/boy_walk/boy walk.0002.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"

[autoload]

g="*res://scenes/global/global.tscn"
ss="*res://scripts/global/save_system.gd"
rc="*res://scenes/global/room_changer_singleton.tscn"
ch="*res://scenes/global/camera_host.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=2
window/size/initial_position=Vector2i(0, -200)
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor]

movie_writer/mjpeg_quality=1.0

[editor_plugins]

enabled=PackedStringArray("res://addons/InstanceDock/plugin.cfg", "res://addons/nklbdev.previewing_parallax_background/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg", "res://addons/wiggly_appendage_2d/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://materials/": "red",
"res://scenes/": "blue",
"res://scripts/": "green",
"res://shaders/": "purple",
"res://sprites/": "yellow",
"res://tools/": "pink",
"res://tools/level_editor/scenes/": "blue",
"res://tools/level_editor/scripts/": "green"
}

